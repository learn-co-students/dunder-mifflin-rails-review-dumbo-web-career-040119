<h1 style="color:blue;">
<%= @employee.errors.full_messages.to_sentence %>
</h1>

<%= form_for @employee do |f| %>
<%= f.label :first_name %>
<%= f.text_field :first_name %>
<br>
<%= f.label :last_name %>
<%= f.text_field :last_name %>
<br>
<%= f.label :title %>
<%= f.text_field :title %>
<br>
<%= f.label  :dog_id %>
<%= f.collection_select :dog_id, Dog.all, :id, :name %>
<br>
<%= f.submit %>
<% end %>
